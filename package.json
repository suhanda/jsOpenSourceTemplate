{
  "name": "js-open-source",
  "version": "0.0.0-semantically-release",
  "description": "Js Worstation Template",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build" : "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "cover": "nyc npm t",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suhanda/jsOpenSourceTemplate.git"
  },
  "keywords": [
    "js",
    "open",
    "source"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Dedi Suhanda <dedi.suhanda@gmail.com> (http://suhanda.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suhanda/jsOpenSourceTemplate/issues"
  },
  "homepage": "https://github.com/suhanda/jsOpenSourceTemplate#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "nyc": "8.3.2",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "webpack": "1.13.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
